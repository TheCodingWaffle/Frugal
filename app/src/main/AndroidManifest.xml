<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frugal.main">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="sensorPortrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DrawerActivity"
            android:label="@string/title_activity_drawer"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".ExpenseActivity"
            android:label="@string/title_activity_expense"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".StatisticsActivity"
            android:label="@string/title_activity_statistics"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".DataEntryActivity"
            android:label="@string/title_activity_data_entry"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".HistoryActivity"
            android:label="@string/title_activity_history"
            android:screenOrientation="sensorPortrait" />
        <activity
            android:name=".HistoryFragment"
            android:label="@string/title_activity_history_fragment"
            android:screenOrientation="sensorPortrait" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".HistoryListAdapter"></activity>
    </application>

</manifest>